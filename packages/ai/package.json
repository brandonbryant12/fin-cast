{
  "name": "@repo/ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./prompts": "./dist/prompts/index.js"
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"],
      "prompts": ["./dist/prompts/index.d.ts"]
    }
  },
  "scripts": {
    "build": "rimraf dist && tsup && tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rimraf dist .turbo node_modules",
    "dev": "tsup --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.3.7",
    "ai": "4.3.2",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "valibot": "*"
  }
} 