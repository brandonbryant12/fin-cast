{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "dev": {
      "persistent": true
    },
    "clean": {
    }
  }
} 